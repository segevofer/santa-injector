{
    "name": "santa injector",
    "version": "0.0.8",
    "manifest_version": 2,
    "description": "Injects stuff related to santa and the editor to your window",
    "content_scripts": [{
        "js": ["background.js"],
        "matches": ["http://*/*", "https://*/*"],
        "all_frames": true
    }],
    "web_accessible_resources": ["santaInjector.js"]
}
